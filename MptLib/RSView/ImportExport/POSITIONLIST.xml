<POSITION_LIST>
  <ANALOG_POSITION>
    <TAG type="A" name="AI\{SHORTNAME}\rMin" description="Регламент MIN" min="{SCALE.Low}" max="{SCALE.HIGH}" units="{UNITS}" dataSource="M" initialValue="{REGLAMENT.LOW}"/>
    <TAG type="A" name="AI\{SHORTNAME}\rMax" description="Регламент MAX" min="{SCALE.Low}" max="{SCALE.HIGH}" units="{UNITS}" dataSource="M" initialValue="{REGLAMENT.HIGH}"/>
    <TAG type="A" name="AI\{SHORTNAME}\sMin" description="Сигнализация MIN" min="{SCALE.Low}" max="{SCALE.HIGH}" units="{UNITS}" dataSource="M" initialValue="{ALARMING.LOW}"/>
    <TAG type="A" name="AI\{SHORTNAME}\sMax" description="Сигнализация MAX" min="{SCALE.Low}" max="{SCALE.HIGH}" units="{UNITS}" dataSource="M" initialValue="{ALARMING.HIGH}"/>
    <TAG type="A" name="AI\{SHORTNAME}\bMin" description="Блокировка по MIN" min="{SCALE.Low}" max="{SCALE.HIGH}" units="{UNITS}" dataSource="M" initialValue="{Blocking.LOW}"/>
    <TAG type="A" name="AI\{SHORTNAME}\bMax" description="Блокировка по MAX" min="{SCALE.Low}" max="{SCALE.HIGH}" units="{UNITS}" dataSource="M" initialValue="{Blocking.HIGH}"/>

    <TAG type="S" name="AI\{SHORTNAME}\Name" description="{Description}" length="200" dataSource="M" initialValue="{NAME}"/>
    
    <TAG type="D" name="AI\{SHORTNAME}\EN" description="Enable" dataSource="D" address="AI[{NUMBER}].EN"  initialValue="1"/>
    <TAG type="A" name="AI\{SHORTNAME}\Current" description="Ток" min="0" max="20" units="мА" dataSource="D" address="AI[{NUMBER}].i"/>  
   
    <TAG type="A" name="AI\{SHORTNAME}\Value" description="{FULLNAME}" min="{SCALE.Low}" max="{SCALE.HIGH}" units="{UNITS}" dataSource="D" address="AI[{NUMBER}].v">
      <DATALOG name ="DLG_SEC"/>
      <DATALOG name ="DLG_TEST"/>
    </TAG>
    
    <TAG type="A" name="AI\{SHORTNAME}\ValueAvr" description="{FULLNAME}" min="{SCALE.Low}" max="{SCALE.HIGH}" units="{UNITS}" dataSource="D" address="AI[{NUMBER}].vAvr">
      <DATALOG name ="DLG_MIN"/>
    </TAG>

    <TAG type="D" name="AI\{SHORTNAME}\Break" description="{FULLNAME}" initialValue="0" dataSource="D"  address="AI[{NUMBER}].break.sgn">
      <ALARM type="On" Label="Обрыв" Severity="4"/>
      <DATALOG name ="DLG_SEC"/>
      <DATALOG name ="DLG_MIN"/>
      <DATALOG name ="DLG_TEST"/>
    </TAG>

    <TAG type="A" name="AI\{SHORTNAME}\State" description="{FULLNAME}" dataSource="D" min="0" max="8" address="AI[{NUMBER}].state">
      <ALARM number="1" threshold="0.9" label="&lt;{Blocking.LOW} {UNITS} (LL)" direction="D" Severity="1"/>
      <ALARM number="2" threshold="1.9" label="&lt;{ALARMING.LOW} {UNITS} (L)" direction="D" Severity="2"/>
      <ALARM number="3" threshold="2.9" label="&lt;{REGLAMENT.LOW} {UNITS} (RL)" direction="D" Severity="3"/>
      <ALARM number="4" threshold="4.9" label="&gt;{REGLAMENT.HIGH} {UNITS} (RH)" direction="I" Severity="3"/>
      <ALARM number="5" threshold="5.9" label="&gt;{ALARMING.HIGH} {UNITS} (H)" direction="I" Severity="2"/>
      <ALARM number="6" threshold="6.9" label="&gt;{Blocking.HIGH} {UNITS} (HH)" direction="I" Severity="1"/>
    </TAG>
    
